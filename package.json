{
  "name": "csl_jobs",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "prestart": "npm run clean && npm run build && npm run resources",
    "resources": "copyfiles src/resources/**.* dist",
    "start": "func start",
    "debug-local": "ts-node src/start.ts ",
    "test": "nyc ts-mocha --paths -p ./tsconfig.json --timeout 10000 --reporter mocha-junit-reporter --reporter spec --exit \"src/**/*.spec.ts\""
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/functions": "^4.0.0-alpha.7",
    "@azure/storage-blob": "^12.15.0",
    "@slack/web-api": "^6.8.1",
    "@types/objects-to-csv": "^1.3.1",
    "archiver": "^5.3.1",
    "archiver-zip-encrypted": "^1.0.11",
    "csv": "^6.3.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "iso8601-duration": "^2.1.1",
    "log": "^6.3.1",
    "log-node": "^8.0.3",
    "memorystream": "^0.3.1",
    "mysql2": "^3.4.3",
    "notifications-node-client": "^7.0.3",
    "objects-to-csv": "^1.3.6",
    "stream-buffers": "^3.0.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/archiver": "^5.3.2",
    "@types/log": "^6.3.0",
    "@types/memorystream": "^0.3.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.x",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@types/stream-buffers": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "chai": "^4.3.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.0",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "main": "dist/src/functions/*.js"
}
